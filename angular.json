{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": { "analytics": false },
  "projects": {
    "interntracker": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/interntracker",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "styles": [
              "src/styles.scss",
              "node_modules/material-icons/iconfont/material-icons.css"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/material-icons/iconfont",
                "output": "assets/material-icons"
              }
            ],
            "scripts": [],
            "allowedCommonJsDependencies": ["@tauri-apps/api"]
          },
          "configurations": {
            "production": {
              "baseHref": "./",
              "deployUrl": "./",
              "budgets": [
                { "type": "initial", "maximumWarning": "10MB", "maximumError": "15MB" },
                { "type": "anyComponentStyle", "maximumWarning": "50kb", "maximumError": "100kb" }
              ],
              "outputHashing": "none",
              "optimization": {
                "scripts": true,
                "styles": { "minify": true, "inlineCritical": false }
              },
              "extractLicenses": false,
              "sourceMap": false,
              "vendorChunk": true,
              "buildOptimizer": true
            },
            "development": {
              "baseHref": "/",
              "deployUrl": "/",
              "optimization": false,
              "outputHashing": "none",
              "extractLicenses": false,
              "sourceMap": true,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "port": 1420 },
          "configurations": {
            "production": { "buildTarget": "interntracker:build:production" },
            "development": { "buildTarget": "interntracker:build:development" }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
